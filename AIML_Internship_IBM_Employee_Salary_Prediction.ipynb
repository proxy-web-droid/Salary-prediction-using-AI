        "\n",
        "# Run Streamlit in the background\n",
        "os.system('streamlit run app.py --server.port 8501 &')\n",
        "\n",
        "# Create a public URL with ngrok\n",
        "public_url = ngrok.connect(8501)\n",
        "print(f\"Streamlit app is live at: {public_url}\")"
